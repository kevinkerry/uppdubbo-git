<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE segment SYSTEM "packetutf8.dtd">
<segment>
	<xmlTag tagName="ReturnForm">
		<String name="ReturnForm"></String>
	</xmlTag>
	<!-- 核心返回日期 -->
	<xmlTag tagName="HostClearDate">
		<Date name="HostClearDate" pattern="yyyyMMdd"></Date>
	</xmlTag>

	<xmlTag tagName="TransDate">
		<Date name="PostDate" pattern="yyyyMMdd"></Date>
	</xmlTag>
	
	<!-- 二维码地址 -->
	<xmlTag tagName="CodeUrl">
		<String name="CodeUrl"></String>
	</xmlTag> 
	<!-- 二维码订单号-->
	<xmlTag tagName="Qrcodeordernbr">
		<String name="Qrcodeordernbr"></String>
	</xmlTag>
	<!-- 支付宝付款账号 -->
	<xmlTag tagName="PayerAcctNbr">
		<String name="PayerAcctNbr"></String>
	</xmlTag>
	<!-- 支付宝实收金额 -->
	<xmlTag tagName="ReceiptAmount">
		<String name="ReceiptAmount"></String>
	</xmlTag>
	<!-- 交易状态 -->
	<xmlTag tagName="TransStatus">
		<String name="TransStatus"></String>
	</xmlTag>
</segment>