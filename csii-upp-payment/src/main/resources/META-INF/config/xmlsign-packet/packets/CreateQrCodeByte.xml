<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE segment SYSTEM "packetutf8.dtd">
<segment>
	<Group name="Message">
		<!-- 交易id -->
		<xmlTag tagName="TransId">
			<String name="TransId" />
		</xmlTag>
		<!-- 一级商户 -->
		<xmlTag tagName="MerchantId">
			<String name="MerNbr" />
		</xmlTag>
		<!-- 商户交易流水号 -->
		<xmlTag tagName="MerSeqNo">
			<String name="MerSeqNbr" />
		</xmlTag>
		<!-- 商户交易时间 -->
		<xmlTag tagName="TransDate">
			<String name="TransDate" />
		</xmlTag>
		<!-- 二维码url地址 -->
		<xmlTag tagName="CodeUrl">
			<String name="CodeUrl" />
		</xmlTag>
		<!-- 二维码信息字符串 -->
		<xmlTag tagName="CodeStreamStr">
			<String name="CodeStreamStr" />
		</xmlTag>
		<!-- 支付接入类型 -->
		<xmlTag tagName="PayAccessType">
			<String name="CodeTypCd" />
		</xmlTag>
		<!-- 支付类型 -->
		<xmlTag tagName="PayType">
			<String name="PayType"/>
		</xmlTag>
		<!-- 应答码 -->
		<xmlTag tagName="RespCode">
			<String name="RespCode" />
		</xmlTag>
		<!-- 应答消息 -->
		<xmlTag tagName="RespMessage">
			<String name="RespMessage" />
		</xmlTag>
	</Group>
</segment>