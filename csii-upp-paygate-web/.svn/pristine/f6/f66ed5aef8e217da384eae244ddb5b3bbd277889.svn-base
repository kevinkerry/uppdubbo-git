@charset "utf-8";
/* CSS Document */

/*公共样式*/
body{ width:100%; height:100%; font-size:12px; color: #333333; font-family:Arial, Helvetica, sans-serif; line-height:1.5em;}
*{ margin:0px; padding:0px; font-family:Arial, "宋体"; font-size:12px;}
p{line-height:1.5em; text-indent:2em;}
ul li{list-style-type:none;}
img{ border:0px; vertical-align:middle;}
a{color:#333333; text-decoration: none;}
a:hover{ text-decoration:underline;}
td{font-family:Arial, "宋体"; font-size:12px; line-height:20px;}

.red{ color:#FF0000;}
.yellow{ color:#f3aa00;}
.blue{ color:#005ed5;}

