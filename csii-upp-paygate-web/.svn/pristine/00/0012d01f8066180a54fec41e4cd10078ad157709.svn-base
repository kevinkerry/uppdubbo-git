@charset "utf-8";
/* CSS Document */

body {margin:0; padding:0; background:#fff; font:normal 12px/20px Arial, Helvetica, sans-serif,"宋体"; color:#333; text-align:center;}
div,span,ul,li,ol,dl,dt,dd,img, form, h1, h2, h3, h4, h5,h6,fieldset  {padding:0; margin:0; border:0;text-align:left;}
table,th,tr,td {font:normal 12px/20px Arial, Helvetica, sans-serif,"宋体";color:#000;border-collapse: collapse}
h1 {padding:0;margin:0 auto;text-align:center;font-weight:normal;font-size:24px;line-height:30px;}
fieldset{border:0; padding:0; margin:0;}
LEGEND {border:0; padding:0; margin:0;}
.clear {clear:both;}
/*  Clearfix Hack:  */
.clearfix:after {content: ".";display: block;height: 0;clear: both;visibility: hidden;}
.clearfix {display: inline-block;}
/* Hides from IE-mac \*/
* html .clearfix { height: 1%;}
.clearfix {display: block;}
/* End hide from IE-mac */

a {font:normal 12px/20px Arial, Helvetica, sans-serif,"宋体"; color:#333; text-decoration:none;}
a:hover {font:normal 12px/20px Arial, Helvetica, sans-serif,"宋体"; color:#f00; text-decoration:underline;}
.fw12 {color:#fff;font-size:14px;font-weight:bold;}
.fl12 {color:#006599;text-decoration:none;}
.fl12 a{color:#006599; text-decoration:none;}
.fl12 a:hover{color:#f00;text-decoration:underline;}
.f12b {color:#006599;text-decoration:underline;}
.f12b a{color:#006599; text-decoration:underline;}
.f12b a:hover{color:#f00;text-decoration:underline;}
.fr12b, .fr12b a{font-weight:bold;color:#c00; text-decoration:none;}
.fr12b a:hover{font-weight:bold;color:#f00;text-decoration:underline;}
.fr12, .fr12 a{color:#f00; text-decoration:none;}
.fr12 a:hover{color:#f00;text-decoration:underline;}
.fr12_2, .fr12_2 a{color:#f00; text-decoration:underline;}
.fr12_2 a:hover{color:#f00;text-decoration:underline;}
.fr12_3, .fr12_3 a{color:#c00; text-decoration:none;}
.fr12_3 a:hover{color:#c00;text-decoration:underline;}
.fr12_4, .fr12_4 a{color:#c00; text-decoration:underline;}
.fr12_4 a:hover{color:#c00;text-decoration:underline;}
.fr14, .fr14 a {font-size:14px; color:#c00; text-decoration:none;}
.fr14 a:hover{font-size:14px; color:#c00;text-decoration:underline;}
.fw12b {font-size:12px; font-weight:bold;color:#fff; text-decoration:none;}
.fg12b, .fg12b a {font-weight: bold;font-size:12px;color:#666;}
.fg12b a:hover{font-weight: bold;font-size:12px;color:#f00;text-decoration:underline;}




/*  ================================= 统一版样式 ======================================  */

.loading {text-align:left; width:300px; padding:2px 10px 2px 110px; position:absolute; top:30px; left:445px; color:#f00; background:url(../images/loading.gif) no-repeat left 8px; visibility:hidden;} /*  loading条样式  */




/*  =========================交易区样式开始=================================  */
/*  交易区查询表格  */
.submitTable {width:600px; margin:0 auto; CLEAR: both; border:0; border-collapse: collapse;}
.submitTable a { font:normal 12px/20px Arial, Helvetica, sans-serif,"宋体"; COLOR: #cd000c; TEXT-DECORATION: underline;}
.submitTable a:hover { font:bold 12px/20px Arial, Helvetica, sans-serif,"宋体"; COLOR: #f00; TEXT-DECORATION: underline;}
.submitTable td {font:normal 12px/20px Arial, Helvetica, sans-serif,"宋体"; color: #000; border-collapse: collapse; border:0; padding:4px 0;}
.submitTable .tdTitle, .submitTable th { font:normal 12px/20px Arial, Helvetica, sans-serif,"宋体"; color: #000; border: #bebebe 1px solid; padding:3px 20px 3px 5px; background: #ededed url(../images/tdTitlebk.gif) repeat-y right; height: 20px; width: 25%; TEXT-ALIGN: right;}
.submitTable .tdValue, .submitTable td { font:normal 12px/20px Arial, Helvetica, sans-serif,"宋体"; color: #000; border: #bebebe 1px solid; padding:3px 5px; HEIGHT: 20px; background: #fff; TEXT-ALIGN: left;}
.submitTable .tdValueError {border: #fff 1px solid; padding:2px 5px; HEIGHT: 25px; BACKGROUND-COLOR: #fff; TEXT-ALIGN: left; }
.submitTable tfoot td {font-size:1px; color: #fff; border:0; padding:0; height:5px; line-height:1px; background:#fff url(../images/myinfo_shadow.gif) repeat-x left top;overflow:hidden;}


/*  交易区查询结果表格  */
.listTabledate {margin:20px auto 10px auto; clear:both; width:100%;} /* 结果列表数据日期容器  */
.listTable {CLEAR: both; border:0; border-collapse: collapse; width:100%;}  /* 结果列表数据样式  */
.trTitle {}
.listTable A {font:normal 12px/20px Arial, Helvetica, sans-serif,"宋体"; COLOR:#f00; TEXT-DECORATION: underline;}
.listTable A:hover {font:bold 12px/20px Arial, Helvetica, sans-serif,"宋体"; color:#f00; font-weight:bold; TEXT-DECORATION: underline;}
.listTable .tdTitle, .listTable th { border:#acacac 1px solid; padding:2px 5px 2px 10px; BACKGROUND:#fff url(../images/listTitlebk.gif) repeat-x bottom; font:bold 12px/20px Arial, Helvetica, sans-serif,"宋体"; HEIGHT: 24px; TEXT-ALIGN: left;}/* 表头样式  */
.listTable .tdValue, .listTable td {border:#acacac 1px solid; padding:2px 5px 2px 10px; COLOR: #000; HEIGHT: 18px; background: #fff; TEXT-ALIGN: left;}
/*  =========================交易区样式结束=================================  */


.submitButton {font-size:12px; font-weight:normal; color:#000; text-align:center; line-height:26px; height:26px; padding:0 15px; cursor:hand; border-width:1px; border-style:solid; border-color:#cdcdcd #8c8c8c #8c8c8c #cdcdcd; background:#c6c6c6 url(../images/submitButtonbk.gif) repeat-x left top;}


.btnBox {margin:20px auto;height:40px;clear:both;text-align:center;}



/*  =========================页面底部样式=================================  */
.footWrap {padding-top:6px;text-align:center;color:#333;border-top:2px solid #e5000d;}









/*  =========================登录页面样式=================================  */
.loginMenu {display:block;width:186px;list-style:none;margin:0 auto;padding:10px 0;background:url(../images/login_leftbg_bg.jpg) repeat-y;}
.loginMenu li {display:block;padding-left:15px;}
.loginMenu li a,
.loginMenu li a:hover
 {display:block;padding-left:24px;height:25px;line-height:25px;background:url(../images/icon_login_fwq.jpg) no-repeat 10px 10px;text-align:left;color:#666;}
.loginMenu li a:hover  {background:url(../images/icon_login_fwqhover.jpg) no-repeat 10px 10px; color:#f00}


.kf a,
.kf a:hover
 {width:97px;height:23px;display:block; background:url(../images/kefu_login1.jpg) no-repeat;}
.kf a:hover{background:url(../images/kefu_login2.jpg) no-repeat;}


.left_kj{
	background: url(../images/icon_login_fwq.jpg);
	background-repeat: no-repeat;
	background-position: 15px 7px;
	text-align: left;
	text-indent: 28px;
	color:#666666;
	height:25px;
}
.left_kj a{
	color:#666666;
}
.left_kj a:hover{
	background: url(../images/icon_login_fwq.jpg);
	background-repeat: no-repeat;
	background-position: 15px 7px;
	color:#CD0104;
}

.loginBtn {
	background-image: url(../images/anniu_login.jpg);
	text-align: center;
	height: 33px;
	width: 87px;
	color: #FFFFFF;
	font-weight: bold;
	border:0;
	cursor:pointer;
}

.ocx_class{
	border:#ffc689 solid 1px; 
	width: 110px; 
	height: 36px;
}

.pay_loginLeftInfo {width:210px;margin:0 auto;padding:10px 12px;background:url(../images/login_leftbg_bg.jpg) repeat-y;}
.pay_loginLeftInfo table {width:100%;margin:0 auto;}
.pay_loginLeftInfo table td {text-align:left;padding-left:2px;}

.errorMsg{ text-align:center; margin:0 auto; color:red; line-height:1.6; font-family: "宋体",Arial,sans-serif; font-size: 120%; }
.paddingBlank { padding-bottom: 100px; }