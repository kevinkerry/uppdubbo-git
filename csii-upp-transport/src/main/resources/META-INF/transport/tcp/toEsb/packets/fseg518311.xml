<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE segment SYSTEM "packetutf8.dtd">
<!-- 富阳贷记卡退款交易 -->
<segment>
	<!-- 转出帐号 -->
	<xmlTag tagName="ACRAAC22">
		<String name="payerAcctNbr" defaultValue=""/>
	</xmlTag>
	<!-- 币种 -->
	<xmlTag tagName="ACRACCYC">
		<String name="currencyCd" defaultValue="" />
	</xmlTag>
	<!-- 转入账号 -->
	<xmlTag tagName="ACRBAC22">
		<String name="payeeAcctNbr" defaultValue=""/>
	</xmlTag>
	<!-- 退货金额 -->
	<xmlTag tagName="ACRBAMT">
		<String name="transAmt" defaultValue=""/>
	</xmlTag>
	<!-- 对账分类编号 -->
	<xmlTag tagName="ACRADZBH">
		<String name="checkNumber" defaultValue=""/>
	</xmlTag>
	<!-- 订单号 -->
	<xmlTag tagName="ACRADDH">
		<String name="ACRADDH" defaultValue=""/>
	</xmlTag>
	<!-- POS编号 -->
	<xmlTag tagName="POS_NUM">
		<String name="POS_NUM" defaultValue="IPP00001"/>
	</xmlTag>
	<!-- 商户编号 -->
	<xmlTag tagName="POS_COD">
		<String name="merNbr" defaultValue="999999999999992"/>
	</xmlTag>
	<!-- 卡密码 -->
	<xmlTag tagName="ACRACHIP">
		<String name="payerCardPwd" defaultValue=""/>
	</xmlTag>
	<!-- 卡有效期 -->
	<xmlTag tagName="ACRALIM">
		<String name="payeeCardExpiredDate" defaultValue=""/>
	</xmlTag>
	<!-- CVV -->
	<xmlTag tagName="ACRACVV">
		<String name="payeeCardCvv2" defaultValue=""/>
	</xmlTag>
	<!-- 前置日期 -->
	<xmlTag tagName="ACRDATE">
		<Date name="transDate" defaultValue="" pattern="yyyyMMdd" />
	</xmlTag>
	<!-- 商户地址 -->
	<xmlTag tagName="ACRADDR">
		<String name="payTypCd" defaultValue=""/>
	</xmlTag>
	<!-- 备用字段 -->
	<xmlTag tagName="ACRABAKE">
		<String name="note" defaultValue=""/>
	</xmlTag>
</segment>