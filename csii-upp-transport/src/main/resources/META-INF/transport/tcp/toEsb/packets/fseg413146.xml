<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE segment SYSTEM "packetutf8.dtd">
	<!-- 富阳提现 -->
<segment>
	<!-- 币种 -->
	<xmlTag tagName="BDRCCYC">
		<String name="currencyCd" defaultValue="CNY"/>
	</xmlTag>
	<!-- 钞汇标志 -->
	<xmlTag tagName="BDRCHSX">
		<String name="cashFlag" defaultValue="1"/>
	</xmlTag>
	<!-- 转出账号 -->
	<xmlTag tagName="BDRAAC22">
		<String name="payerAcctNbr" defaultValue=""/>
	</xmlTag>
	<!-- 转入账号 -->
	<xmlTag tagName="BDRBAC22">
		<String name="payeeAcctNbr" defaultValue=""/>
	</xmlTag>
	<!-- 现转标志 -->
	<xmlTag tagName="BDRTXTP">
		<String name="BDRTXTP" defaultValue="2" />
	</xmlTag>
	<!-- 处理类别 -->
	<xmlTag tagName="BDRACLLB">
		<String name="processMode" defaultValue=""/>
	</xmlTag>
	<!-- 订单号 -->
	<xmlTag tagName="BDRADDH">
		<String name="transNbr" defaultValue=""/>
	</xmlTag>
	<!-- 是否检验支取方式 -->
	<xmlTag tagName="BDRCHKZQ">
		<String name="checkPwdFlag" defaultValue="0" />
	</xmlTag>
	<!-- 支取方式 -->
	<xmlTag tagName="BDRDRWT">
		<String name="withFlag" defaultValue="1"/>
	</xmlTag>
	<!-- 支取密码 -->
	<xmlTag tagName="BDRPWDX1">
		<String name="payerCardPwd" defaultValue=""/>
	</xmlTag>
	<!-- 支取证件种类 -->
	<xmlTag tagName="BDRCFTP1">
		<String name="payerIdTypCd" defaultValue=""/>
	</xmlTag>
	<!-- 支取证件号码 -->
	<xmlTag tagName="BDRCFNO1">
		<String name="payerIdNbr" defaultValue=""/>
	</xmlTag>
	<!-- 交易金额 -->
	<xmlTag tagName="BDRAMT1">
		<String name="transAmt" defaultValue=""/>
	</xmlTag>
	<!-- 摘要代码 -->
	<xmlTag tagName="BDRNTCD1">
		<String name="BDRNTCD1" defaultValue=""/>
	</xmlTag>
	<!-- 备注 -->
	<xmlTag tagName="BDRRMARK">
		<String name="note" defaultValue=""/>
	</xmlTag>
	<!-- 对账分类编号 -->
	<xmlTag tagName="BDRFLBH">
		<String name="checkNumber" defaultValue=""/>
	</xmlTag>
	<!-- 前置营业日 -->
	<xmlTag tagName="BDRDATE"> 
		<Date name="transDate" defaultValue="" pattern="yyyyMMdd" />
	</xmlTag>
	<!-- 转出账户回单标志 -->
	<xmlTag tagName="BDRAHDBZ">
		<String name="BDRAHDBZ" defaultValue="1"/>
	</xmlTag>
	<!-- 转入账户回单标志 -->
	<xmlTag tagName="BDRBHDBZ">
		<String name="BDRBHDBZ" defaultValue="1"/>
	</xmlTag>
	<!-- 备用 -->
	<xmlTag tagName="BDRMARK">
		<String name="payeeName" defaultValue=""/>
	</xmlTag>
	
</segment>