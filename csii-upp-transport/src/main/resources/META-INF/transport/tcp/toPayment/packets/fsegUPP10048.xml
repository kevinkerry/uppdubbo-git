<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE segment SYSTEM "packetutf8.dtd">
<segment>
	<!-- 商户号 -->
	<xmlTag tagName="MerNbr">
		<String name="merNbr" defaultValue="" />
	</xmlTag>
	<!-- 用户编号 -->
	<xmlTag tagName="MerSeqNbr">
		<String name="merSeqNbr" defaultValue="" />
	</xmlTag>
	<!-- 商户交易时间 -->
	<xmlTag tagName="TransDate">
		<String name="merTransDateTime" />
	</xmlTag>
	<!-- 币种 -->
	<!--二维码字节 -->
	<xmlTag tagName="CodeStreamStr">
		<String name="codeStreamStr" defaultValue="" />
	</xmlTag> 
	<!--二维码地址 -->
	<xmlTag tagName="CodeUrl">
		<String name="codeUrl" defaultValue="" />
	</xmlTag>
	<!--二维码类型 -->
	<xmlTag tagName="CodeTypCd">
		<String name="codeTypCd" defaultValue="" />
	</xmlTag>
	<!--接入类型-->
	<xmlTag tagName="PayType">
		<String name="payType" defaultValue="" />
	</xmlTag>  
	<!--错误码 -->
	<xmlTag tagName="RespCode">
		<String name="respCode" defaultValue="" />
	</xmlTag>
	<!--错误信息-->
	<xmlTag tagName="RespMessage">
		<String name="respMessage" defaultValue="" />
	</xmlTag>
</segment>