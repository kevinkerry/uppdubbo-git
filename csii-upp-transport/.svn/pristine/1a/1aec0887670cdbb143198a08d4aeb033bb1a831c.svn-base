<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE segment SYSTEM "packetutf8.dtd">
	<!-- 贷记卡卡信息查询 -->
<segment>
	<!-- 卡号 -->
	<xmlTag tagName="CARDNBR">
		<String name="payerAcctNbr" defaultValue=""/>
	</xmlTag>
	<!-- 行号 -->
	<xmlTag tagName="BNKNBR">
		<String name="BNKNBR" defaultValue="1429" />
	</xmlTag>
	<!-- 查询方式 -->
	<xmlTag tagName="INQOPT">
		<String name="INQOPT" defaultValue="1" />
	</xmlTag>
	<xmlTag tagName="BRN_NO">
		<String name="BRN_NO" defaultValue="999000" />
	</xmlTag>
	<xmlTag tagName="OPE_NO">
		<String name="OPE_NO" defaultValue="999S40" />
	</xmlTag>
	<xmlTag tagName="SEQNO">
		<String name="transNbr" defaultValue=""/>
	</xmlTag>
	<xmlTag tagName="SOURCE">
		<String name="SOURCE" defaultValue="TB" />
	</xmlTag>
	<!-- 是否检查密码标志 
	<xmlTag tagName="PIN_FLAG">
		<String name="PIN_FLAG" defaultValue="3" />
	</xmlTag> -->
	<!-- 选项 -->
	<xmlTag tagName="OPTION">
		<String name="OPTION" defaultValue="1" />
	</xmlTag>
</segment>
