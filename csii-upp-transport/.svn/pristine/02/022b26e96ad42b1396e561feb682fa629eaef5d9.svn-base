	<bean id="eaccount.messageFactory" class="com.csii.pe.channel.ws.AxiomSoapMessageFactory2">
		<property name="payloadCaching" value="true" />
		<property name="charsetEncoding" value="UTF-8" />
	</bean>
	
	<bean id="eaccount.serviceManager" class="com.csii.pe.dynamic.channel.ws.DynamicServiceManager">
		<property name="identityValue" value="*" />
		<property name="importServiceDefinition" value="true" />
		<property name="useSdo" value="true" />
		<property name="useToDataDefault" value="false" />
		<property name="useToSdoDefault" value="false" />
		<property name="outputNull" value="false" />
		<property name="dataFlatting" value="false" />
	</bean>

	<bean id="eaccount.sdoMarshaller" class="com.csii.upp.transport.ws.SdoMarshaller">
		<property name="serviceManager" ref="eaccount.serviceManager" />
		<property name="serverSide" value="false" />
		<property name="validation" value="false" />
		<property name="validationOnMarshalling" value="false" />
	</bean>

	<bean id="eaccount.webServiceTransport" class="com.csii.pe.channel.ws.client.CustomWebServiceGateway">
		<constructor-arg ref="eaccount.messageFactory" />
		<property name="serviceManager" ref="eaccount.serviceManager" />
		<property name="marshaller" ref="eaccount.sdoMarshaller" />
		<property name="identityName" value="transCode" />
		<property name="identityMapping" ref="eaccount.identityMapping" />
		<property name="defaultUri" value="http://localhost:8080/ws/" />
		<property name="interceptors">
			<list>
				<bean class="com.csii.pe.channel.ws.client.PayloadLoggerClientInterceptor" />
			</list>
		</property>
		<property name="messageSender">
			<bean
				class="org.springframework.ws.transport.http.CommonsHttpMessageSender">
				<property name="connectionTimeout" value="6000" />
				<property name="readTimeout" value="0" />
			</bean>
		</property>
	</bean>
</beans>
