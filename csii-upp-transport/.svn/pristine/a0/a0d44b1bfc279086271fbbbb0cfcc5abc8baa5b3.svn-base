<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE segment SYSTEM "packetutf8.dtd">
<segment>
	<!-- 付款账户号 -->
	 <xmlTag tagName="PayerAcctNbr"><String name="payerAcctNbr"/></xmlTag>
	<!-- 付款账户类型 -->
	 <xmlTag tagName="PayerCardTypCd"><String name="payerCardTypCd"/></xmlTag>
	<!-- 手机号 -->
	 <xmlTag tagName="PayerPhoneNo"><String name="payerPhoneNo"/></xmlTag>
	<!-- 客户号 -->
	 <xmlTag tagName="CustCifNbr"><String name="custCifNbr"/></xmlTag>
	<!-- 商户号 -->
	 <xmlTag tagName="MerNbr"><String name="merNbr"/></xmlTag>
	<!-- 商户流水号 -->
	 <xmlTag tagName="MerSeqNbr"><String name="merSeqNbr"/></xmlTag>
	<!-- 交易金额 -->
	 <xmlTag tagName="TransAmt"><String name="transAmt"/></xmlTag>
	<!-- 商户时间戳 -->
	 <xmlTag tagName="MerTransDateTime"><String name="merTransDateTime" /></xmlTag>
	<!-- 币种 -->
	 <xmlTag tagName="CurrencyCd"><String name="currencyCd"/></xmlTag>
	<!-- 客户邮箱 -->
	 <xmlTag tagName="CustEmail"><String name="custEmail"/></xmlTag>
	<!-- 商户链接 -->
	 <xmlTag tagName="MerUrl"><String name="merUrl"/></xmlTag>
	<!-- 商户链接1 -->
	 <xmlTag tagName="MerUrl1"><String name="merUrl1"/></xmlTag>
	<!-- 支付地址 -->
	 <xmlTag tagName="PayIp"><String name="payIp"/></xmlTag>
	<!-- 附加信息 -->
	 <xmlTag tagName="MsgExt"><String name="msgExt"/></xmlTag>
	<!-- 信用卡CVV2 -->
	 <xmlTag tagName="PayerCardCvv2"><String name="payerCardCvv2"/></xmlTag>
	<!-- 信用卡有效年月 -->
	 <xmlTag tagName="PayerCardExpiredDate"><String name="payerCardExpiredDate"/></xmlTag>
	<!-- 付款账户机构 -->
	 <xmlTag tagName="PayerAcctDeptNbr"><String name="payerAcctDeptNbr"/></xmlTag>
	<!-- 付款账户名称 -->
	 <xmlTag tagName="PayerAcctName"><String name="payerAcctName"/></xmlTag>
	 <!-- 网银支付类型 -->
	 <xmlTag tagName="CyberTypCd"><String name="cyberTypCd"/></xmlTag>
	 <!-- 他行快捷支付，短信支付的流水号 -->
	 <xmlTag tagName="SmsInnerFundTransNbr"><String name="smsInnerFundTransNbr"/></xmlTag>
	 <!-- 他行快捷支付，短信支付的流水时间-->
	 <xmlTag tagName="SendUnionPayTime"><String name="sendUnionPayTime"/></xmlTag>
	 <!--行码-->
	 <xmlTag tagName="PayerBankNbr"><String name="payerBankNbr"/></xmlTag>
	 <!--发送银联的前端回调地址-->
	 <xmlTag tagName="FrontCallBackUrl"><String name="frontCallBackUrl"/></xmlTag>
	<!-- 子商户交易列表 -->
	 <xmlTag tagName="MerTransList">
	    <idxField name="merTransList" maxLoop="9999999">
			<Group name="MerTransDetail">
		   		<xmlTag><String name="SubMerchantId" /></xmlTag>
	   		    <xmlTag><String name="SubMerSeqNo" /></xmlTag>
	   		    <xmlTag><String name="ProductInfo" /></xmlTag>
	   		    <xmlTag><String name="SubTransAmt" /></xmlTag>
	   		    <xmlTag><String name="SubMerDateTime" /></xmlTag>
				<xmlTag><String name="SubMerImport"/></xmlTag>
			</Group>
		</idxField>
	</xmlTag>
</segment>