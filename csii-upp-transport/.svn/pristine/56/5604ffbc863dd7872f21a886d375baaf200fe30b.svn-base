<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE segment SYSTEM "packetutf8.dtd">
<segment>
    <!-- 卡号 -->
	<xmlTag tagName="PayerAcctNbr">
		<String name="payerAcctNbr" defaultValue="" />
	</xmlTag>
	<!-- 手机号 -->
	<xmlTag tagName="PayerPhoneNo">
		<String name="payerPhoneNo" defaultValue="" />
	</xmlTag>
	<!-- 签约号 -->
	<xmlTag tagName="SignNbr">
		<String name="signNbr" />
	</xmlTag>
	<!-- 签约类型 -->
	<xmlTag tagName="SignTypCd">
		<String name="signTypCd"/>
	</xmlTag>
	<!-- 单笔交易限额 -->
	<xmlTag tagName="PerTransLimit">
		<String name="perTransLimit"/>
	</xmlTag>
	<!-- 日累计交易限额 -->
	<xmlTag tagName="PerDayLimit">
		<String name="perDayLimit"/>
	</xmlTag>
</segment>