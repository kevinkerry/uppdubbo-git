<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE segment SYSTEM "packetutf8.dtd">
<segment>
	<!-- 付款账户号 -->
	 <xmlTag tagName="PayerAcctNbr"><String name="payerAcctNbr"/></xmlTag>
	 
	  <xmlTag tagName="PayerAcctOrgDeptNbr"><String name="payerAcctOrgDeptNbr"/></xmlTag>
	 <!-- 短信验证码 -->
	 <xmlTag tagName="SmsCode"><String name="smsCode"/></xmlTag>
	<!-- 付款账户类型 -->
	 <xmlTag tagName="PayerCardTypCd"><String name="payerCardTypCd"/></xmlTag>
	<!-- 手机号 -->
	 <xmlTag tagName="PayerPhoneNo"><String name="payerPhoneNo"/></xmlTag>
	<!-- 客户号 -->
	 <xmlTag tagName="CustCifNbr"><String name="custCifNbr"/></xmlTag>
	<!-- 商户号 -->
	 <xmlTag tagName="MerNbr"><String name="merNbr"/></xmlTag>
	<!-- 商户流水号 -->
	 <xmlTag tagName="MerSeqNbr"><String name="merSeqNbr"/></xmlTag>
	 <!-- 活动字段-->
	 <xmlTag tagName="ActMemo"><String name="actMemo"/></xmlTag>
	<!-- 交易金额 -->
	 <xmlTag tagName="TransAmt"><String name="transAmt"/></xmlTag>
	<!-- 商户时间戳 -->
	 <xmlTag tagName="MerTransDateTime"><String name="merTransDateTime" /></xmlTag>
	<!-- 币种 -->
	 <xmlTag tagName="CurrencyCd"><String name="currencyCd"/></xmlTag>
	<!-- 客户邮箱 -->
	 <xmlTag tagName="CustEmail"><String name="custEmail"/></xmlTag>
	<!-- 商户链接 -->
	 <xmlTag tagName="MerUrl"><String name="merUrl"/></xmlTag>
	<!-- 商户链接1 -->
	 <xmlTag tagName="MerUrl1"><String name="merUrl1"/></xmlTag>
	<!-- 支付地址 -->
	 <xmlTag tagName="PayIp"><String name="payIp"/></xmlTag>
	<!-- 附加信息 -->
	 <xmlTag tagName="MsgExt"><String name="msgExt"/></xmlTag>
	<!-- 密码 -->
	 <xmlTag tagName="PayerCardPwd"><String name="payerCardPwd"/></xmlTag>
	<!-- 信用卡CVV2 -->
	 <xmlTag tagName="PayerCardCvv2"><String name="payerCardCvv2"/></xmlTag>
	<!-- 信用卡有效年月 -->
	 <xmlTag tagName="PayerCardExpiredDate"><String name="payerCardExpiredDate"/></xmlTag>
	<!-- 付款账户机构 -->
	 <xmlTag tagName="PayerAcctDeptNbr"><String name="payerAcctDeptNbr"/></xmlTag>
	<!-- 付款账户名称 -->
	<xmlTag tagName="PayerAcctName"><String name="payerAcctName"/></xmlTag>
	 <!-- 网银支付类型 -->
	 <xmlTag tagName="CyberTypCd"><String name="cyberTypCd"/></xmlTag>
	 <!-- 他行快捷支付，短信支付的流水号 -->
	 <xmlTag tagName="SmsInnerFundTransNbr"><String name="smsInnerFundTransNbr"/></xmlTag>
	 <!-- 他行快捷支付，短信支付的流水时间-->
	 <xmlTag tagName="SendUnionPayTime"><String name="sendUnionPayTime"/></xmlTag>
	 <!--行码-->
	 <xmlTag tagName="PayerBankNbr"><String name="payerBankNbr"/></xmlTag>
	 <!--发送银联的前端回调地址-->
	 <xmlTag tagName="FrontCallBackUrl"><String name="frontCallBackUrl"/></xmlTag>
	 <!-- 本次消费积分金额 -->
	<xmlTag tagName="DeductAmt"><BigDecimal name="deductAmt" /></xmlTag>
	<!-- 业务类型 -->
	<xmlTag tagName="BizType"><String name="bizType" /></xmlTag>
	<!-- 请求类型 -->
	<xmlTag tagName="RequestModel"><String name="requestModel" /></xmlTag>
	<!-- 代收机构编码-->
	<xmlTag tagName="TradeCode"><String name="tradeCode" /></xmlTag>
	<!-- 子商户交易列表 -->
	 <!-- 行社号-->
	 <xmlTag tagName="BranchNo"><String name="branchNo" /></xmlTag>
	 <!-- 客户内码-->
	 <xmlTag tagName="ClientNo"><String name="clientNo" /></xmlTag>
	 <!-- 积分标识-->
	 <xmlTag tagName="InteralFlag"><String name="interalFlag" /></xmlTag>
	 <!-- 扫码模式-->
	 <xmlTag tagName="ScanCodeMode"><String name="scanCodeMode" /></xmlTag>
	 <!-- 二维码类型-->
	 <xmlTag tagName="CodeTypCd"><String name="codeTypCd" /></xmlTag>
	 <!-- 打折金额-->
	 <xmlTag tagName="Discountableamt"><BigDecimal name="discountableamt" /></xmlTag> 
	 <!-- 是否支持信用卡 -->
	 <xmlTag tagName="IsCredit"><String name="isCredit"/></xmlTag>
	 <!-- 订单开始时间 -->
	 <xmlTag tagName="TimeStart"><String name="timeStart"/></xmlTag>
	 <!-- 订单截止时间 -->
	 <xmlTag tagName="TimeExpire"><String name="timeExpire"/></xmlTag>
	 <!-- 终端IP-->
	 <xmlTag tagName="CustomerIp"><String name="customerIp"/></xmlTag>
	 <!-- 商品信息列表-->
	 <xmlTag tagName="ProductInfoDetail"><String name="productInfoDetail"/></xmlTag>
	 <!-- 商品编号-->
	 <xmlTag tagName="ProductId"><String name="productId"/></xmlTag>
	 <!-- 商品描述 -->
	 <xmlTag tagName="ProBody"><String name="proBody" /></xmlTag>
	 <!-- 商品标题-->
	 <xmlTag tagName="Subject"><String name="subject" /></xmlTag>
	 <!-- 商户操作号-->
	 <xmlTag tagName="OperatorId"><String name="operatorId" /></xmlTag>
	 <!-- 商户门店号-->
	 <xmlTag tagName="StoreId"><String name="storeId" /></xmlTag>
	 <!-- 微信终端号-->
	 <xmlTag tagName="TermId"><String name="termId" /></xmlTag>
	 <!-- 允许付款时间-->
	 <xmlTag tagName="TimeoutExpress"><String name="timeoutExpress" /></xmlTag>
	 <!-- 支付宝门店号-->
	 <xmlTag tagName="AlipayStoreId"><String name="alipayStoreId" /></xmlTag>
	 <!-- 支付场景-->
	 <xmlTag tagName="Scene"><String name="scene" /></xmlTag>
	 <!-- 支付授权码-->
	 <xmlTag tagName="AuthCode"><String name="authCode" /></xmlTag>
	 <!-- 商品信息-->
	 <xmlTag tagName="GoodsDetail"><String name="goodsDetail" /></xmlTag>
	 <!-- 子商户交易列表 -->
		
	 <xmlTag tagName="CifNo"><String name="cifNo" /></xmlTag>
	 <xmlTag tagName="Term"><String name="term" /></xmlTag>
	
	 <xmlTag tagName="MerTransList">
	    <idxField name="merTransList" maxLoop="9999999">
			<Group name="MerTransDetail">
		   		<xmlTag><String name="SubMerchantId" /></xmlTag>
	   		    <xmlTag><String name="SubMerSeqNo" /></xmlTag>
	   		    <xmlTag><String name="ProductInfo" /></xmlTag>
	   		    <xmlTag><String name="SubTransAmt" /></xmlTag>
	   		    <xmlTag><String name="SubMerDateTime" /></xmlTag>
				<xmlTag><String name="SubMerImport"/></xmlTag>
			</Group>
		</idxField>
	</xmlTag> 
	
</segment>