#==========================begin 批量系统服务配置==========================#
seqnbr.workerId=1
seqnbr.datacenterId=0
#TCP服务IP和端口
batch.tcp.id=UPPBATCHXML
batch.tcp.name=Inbound-UPPBATCHXML-Server
batch.tcp.host=localhost
batch.tcp.port=18003
batch.tcp.timeout=30000

#提供者服务端口
dubbo.protocol.port=20882
dubbo.protocol.dubbo.threadpool=fixed
dubbo.protocol.dubbo.threads=300
dubbo.protocol.dubbo.accepts=300
#提供者服务超时时间
dubbo.provider.timeout=60000
#消费者服务超时时间
dubbo.consumer.timeout=120000

#批量扫描需要定时运行队列(时间单位:毫秒;值暂定)
batch.scanSchedulerRunQue.delay=10000
batch.scanSchedulerRunQue.period=300000

#批量扫描需要文件触发运行队列(时间单位:毫秒;值暂定)
batch.scanFileTriggerRunQue.delay=10000
batch.scanFileTriggerRunQue.period=300000


