				<prop key="{http://com.csii.bank.pay/online}XMPAY">XMPAY</prop>
				<prop key="{http://com.csii.bank.pay/online}CCCPAY">CCCPAY</prop>
				<prop key="{http://com.csii.bank.pay/online}RTCT">RTCT</prop>
