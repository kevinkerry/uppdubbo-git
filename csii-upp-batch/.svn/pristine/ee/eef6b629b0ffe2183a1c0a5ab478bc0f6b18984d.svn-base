	<!-- 小额对账文件模板 -->
	<file delimiter="|" id="beps" prefix="ZXDZ_BEPS_" suffix=".dat"
		lineSeparator="\n" stringSymbol="1" encoding="GBK">
	    <field name="direction" />  <!-- 往来标志IOFlag -->
		<field name="CDFlag" />  <!-- 借贷标志 CDFlag-->
		<field name="innerrtxnnbr" />  <!-- 交易流水号SysLog -->
		<field name="rtxndate" />  <!-- 交易日期RegDat -->
		<field name="ActDat" />  <!-- 主机记账日期ActDat  -->
		<field name="HLogNo" />  <!--主机记账流水HLogNo -->
		<field name="payeracctnbr" />  <!--付款人账号PyrAct -->
		<field name="payername" />  <!--付款人名称PyeNam -->
		<field name="send_settle_bank" />  <!--发起清算行行号PyrSetBk -->
		<field name="payeeacctnbr" />  <!--收款人账号PyeAct -->
		<field name="payeename" />  <!--收款人名称PyeNam -->
		<field name="rcv_settle_bank" /> <!--  接收清算行行号PyeSetBk  -->
		<field name="currency" />  <!--交易币种CcyCod -->
		<field name="transamt" />  <!--金额TRAN_AMT -->	
		<field name="rtxnstate" />  <!--金额 -->
		<field name="id" />  <!-- 报文标识 -->
	</file>
	
	<!--银联对账文件模板 -->
	<file id="unionpayds1" prefix="unionpayds1" skipLine="" delimiter=""
		suffix="" lineSeparator="\n" stringSymbol="1" encoding="gbk">
		<field name="innerrtxntyp" length="3"/><!-- 交易代码 -->
		<field name="blank" length="1"/><!-- 空格分隔符 -->
		<field name="acqInsCode" length="11" parser="rigthTrim"/><!-- 代理机构标识码 -->
		<field name="blank" length="1"/><!-- 空格分隔符 -->
		<field name="sndInsCode" length="11" parser="rigthTrim"/><!-- 发送机构标识码 -->
		<field name="blank" length="1"/><!-- 空格分隔符 -->
		<field name="downrtxnnbr" length="6" /><!-- 系统跟踪号 -->
		<field name="blank" length="1"/><!-- 空格分隔符 -->
		<field name="rtxndate" length="10" parser="addRightYear"/><!-- 交易传输时间 -->
		<field name="blank" length="1"/><!-- 空格分隔符 -->
		<field name="payeracctnbr" length="19" parser="rigthTrim"/><!-- 帐号-->
		<field name="blank" length="1"/><!-- 空格分隔符 -->
		<field name="transamt" length="12" parser="formatAmt" decimal="2"/><!-- 交易金额 -->
		<field name="blank" length="1"/><!-- 空格分隔符 -->
		<field name="merCatCode" length="4" /><!-- 商户类别 -->
		<field name="blank" length="1"/><!-- 空格分隔符 -->
		<field name="termType" length="2"/><!-- 终端类型 -->
		<field name="blank" length="1"/><!-- 空格分隔符 -->
		<field name="queryId" length="21"/><!-- 查询流水号 -->
		<field name="blank" length="1"/><!-- 空格分隔符 -->
		<field name="payTypeOld" length="2" parser="rigthTrim"/><!-- 支付方式（旧） -->
		<field name="blank" length="1"/><!-- 空格分隔符 -->
		<field name="innerrtxnnbr" length="32" parser="rigthTrim"/><!-- 商户订单号 -->
		<field name="blank" length="1"/><!-- 空格分隔符 -->
		<field name="payCardType" length="2"/><!-- 支付卡类型 -->
		<field name="blank" length="1"/><!-- 空格分隔符 -->
		<field name="origTraceNo" length="6" parser="rigthTrim"/><!--原始交易的系统跟踪号 -->
		<field name="blank" length="1"/><!-- 空格分隔符 -->
		<field name="origTxnTime" length="10" parser="addYear"/><!-- 原始交易日期时间 -->
		<field name="blank" length="1"/><!-- 空格分隔符 -->
		<field name="merFee" length="13" parser="formatSymbolAmt" decimal="2"/><!-- 商户手续费 -->
		<field name="blank" length="1"/><!-- 空格分隔符 -->
		<field name="settAmt" length="13" parser="formatSymbolAmt" decimal="2"/><!-- 结算金额(取值为“交易金额”与“商户手续费”的扎差) -->
		<field name="blank" length="1"/><!-- 空格分隔符 -->
		<field name="payType" length="4" /><!-- 支付方式 -->
